{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "semi":[1, "always"],
    "quotes": [1, "double"],
    "no-constant-condition":"off",
    "no-empty":"warn",
    "no-empty-function":"warn",
    "@typescript-eslint/no-empty-function":"off"
  }
}